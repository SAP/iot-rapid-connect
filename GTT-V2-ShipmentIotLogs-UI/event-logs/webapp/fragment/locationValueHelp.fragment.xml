<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Data"
		title="{requestModel>/title}"
		liveChange=".handleSearchControl"
        confirm=".controlVHConfirmKey"
		cancel=".controlVHConfirmKey"
		showClearButton="false"
		items="{
			path : 'requestModel>/valueHelp'
		}">
		<columns>
			<Column width="40%">
				<header>
					<Text text="Location Alternative Key" />
				</header>
			</Column>
			<Column >
				<header>
					<Text text="External ID" />
				</header>
			</Column>
      <Column >
				<header>
					<Text text="Location Description" />
				</header>
			</Column>
      <Column >
				<header>
					<Text text="Logical System" />
				</header>
			</Column>
		</columns>
        <ColumnListItem selected="{requestModel>selected}">
			<cells>
        <Text text="{requestModel>key}"/>
				<Text text="{requestModel>id}"/>
				<Text text="{requestModel>description}" />
				<Text text="{requestModel>system}" />
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>