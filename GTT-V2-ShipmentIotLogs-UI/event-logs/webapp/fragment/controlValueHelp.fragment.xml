<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		noDataText="No Data"
		title="{requestModel>/title}"
		liveChange=".handleSearchControl"
        confirm=".controlVHConfirm"
		cancel=".controlVHConfirm"
		showClearButton="false"
		items="{
			path : 'requestModel>/valueHelp'
		}">
		<columns>
			<Column >
				<header>
					<Text text="id" />
				</header>
			</Column>
			<Column >
				<header>
					<Text text="description" />
				</header>
			</Column>
		</columns>
        <ColumnListItem selected="{requestModel>selected}">
			<cells>
				<Text text="{requestModel>id}"/>
				<Text text="{requestModel>description}" />
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>