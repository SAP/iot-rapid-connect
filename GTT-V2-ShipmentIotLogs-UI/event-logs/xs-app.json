{
  "welcomeFile": "/index.html",
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/shipmentLogTest/(.*)$",
      "target": "/$1",
      "destination": "lbn-iot-gtt-sst-eventlogs-api",
      "scope": ["$XSAPPNAME.sst.r"]
    },
    {
      "source": "^(.*)$",
      "target": "$1",
      "service": "html5-apps-repo-rt",
      "authenticationType": "xsuaa"
    }
  ]
}